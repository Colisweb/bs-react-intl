{
  "name": "bs-react-intl-extractor",
  "version": "0.4.0",
  "description": "Message extractor for bs-react-intl",
  "license": "MIT",
  "esy": {
    "build": [["refmterr", "jbuilder", "build"]],
    "install": ["esy-installer"],
    "buildsInSource": "_build"
  },
  "scripts": {
    "format": "refmt --in-place -w 120 `find bin lib -name \"*.re\"`",
    "setVersion": "node scripts/setVersion.js",
    "zip": "esy build && node scripts/createZip.js"
  },
  "dependencies": {
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@esy-ocaml/reason": "^3.2.0",
    "@opam/jbuilder": "^1.0.0-beta14",
    "@opam/yojson": "^1.4.1",
    "refmterr": "^3.0.4"
  },
  "resolutions": {
    "**/@opam/result": "1.2.0"
  },
  "peerDependencies": {
    "ocaml": "~4.6.1"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.6.1"
  }
}
